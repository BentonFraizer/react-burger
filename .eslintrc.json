{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "plugin:@typescript-eslint/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "undefined": 0,
    "linebreak-style": 0,
    "quotes": ["error", "single"],
    "comma-dangle": "off",
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "object-curly-newline": "off"
  },
  "parser": "@typescript-eslint/parser"
}
